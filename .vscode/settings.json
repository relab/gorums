{
    "protoc": {
        "path": "/usr/local/bin/protoc",
        "compile_on_save": false,
        "options": [
            "--proto_path=${workspaceRoot}",
            "--proto_path=${workspaceRoot}/cmd/protoc-gen-gorums/dev",
        ]
    },
    "cSpell.words": [
        "Ansible",
        "Erevik",
        "ICDCS",
        "Jehl",
        "Mallocs",
        "Paxos",
        "QCQF",
        "Tormod",
        "calltypes",
        "cpuprofile",
        "golangci",
        "memprofile"
    ],
    "go.lintTool": "golangci-lint",
    "go.lintFlags": [
        "--fast",
        "--exclude-use-defaults EXC0001"
    ]
}